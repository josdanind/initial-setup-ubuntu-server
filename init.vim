syntax on

set encoding=UTF-8
set mouse=r
set clipboard=unnamedplus
set timeoutlen=250

set number
set relativenumber

set expandtab
set sw=4
set smartindent

" --// Plugins
source /home/root/.config/nvim/vim-plug/pluggins.vim

" --// Keyboard shortcut
source /home/root/.config/nvim/keybinds.vim

" --// COC default config
source /home/root/.config/nvim/coc.vim


